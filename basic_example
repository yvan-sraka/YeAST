#! clang++ $^ -Wall -Werror -pedantic -std=c++11 -o TEST

#include <iostream>
#include <string>

int main()
{
    std::cout << "Tiny Server !" << std::endl;
    std::cout << "-------------" << std::endl;
    std::string pwd = "#! /bin/sh $^ #! /bin/sh
                echo "pwd"
            !#
            !#";
    std::cout << pwd << std::endl;
    return 0;
}

!#

